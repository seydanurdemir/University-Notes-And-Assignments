<ul class="list-group">
    <a href="/branch/add" class="list-group-item list-group-item-action active">+ Yeni Şube Ekle</a>
    {{#each branchs}}
        <a class="list-group-item list-group-item-action" href="/branch/edit/{{this.id}}">
            Şube Kodu: <b>{{this.branch_code}}</b><br>
            Şube Adı: <b>{{this.branch_name}}</b><br>
        </a>
    {{/each}}
</ul>